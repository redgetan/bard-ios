# Uncomment this line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'

target 'Bard' do
  use_frameworks!

  pod 'Player'
  pod 'RealmSwift'
  pod 'Alamofire', '~> 3.4'
  pod 'SwiftyDrop', '~>2.0'
  pod 'KeychainAccess'
  pod 'HanekeSwift'

  # crash reporting
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Instabug'

  # analytics
  pod 'Firebase'
  pod 'Mixpanel'


end
